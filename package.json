{
  "name": "the-interactive-gallery",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build",
    "prisma:migrate": "npx prisma migrate dev --schema=src/server/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=src/server/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=src/server/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.1",
    "vite": "^6.3.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
